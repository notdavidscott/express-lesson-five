
<h1>Add a new artist!</h1>

<form action="/artists" method="POST">
    <div>
        <label for="name">Name: </label>
        <input type="text" name="name">
    </div>
    <div>
        <label for="name">Date Formed: </label>
        <input type="text" name="dateFormed">
    </div>
    <div>
        <button type="submit">Submit</button>
    </div>
</form>

<h2>Artists:</h2>

{{#each artists}}
<h1>{{Name}}</h1>
{{/each}}